{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "workbench.colorCustomizations": {
    "statusBar.background": "#af4670",
    "statusBarItem.hoverBackground": "#883a54",
    "statusBar.foreground": "#f0d6e8",
    "activityBar.activeBackground": "#e9aad9",
    "activityBar.activeBorder": "#e24153",
    "activityBar.background": "#f0d6e8",
    "activityBar.foreground": "#e24153",
    "activityBar.inactiveForeground": "#3d445199",
    "activityBarBadge.background": "#e24153",
    "activityBarBadge.foreground": "#f9fafb",
    "titleBar.activeBackground": "#af4670",
    "titleBar.activeForeground": "#f0d6e8",
    "titleBar.inactiveBackground": "#af4670",
    "titleBar.inactiveForeground": "#f0d6e8"
  },
  "editor.tabSize": 2,
  "css.validate": false,
  "prettier.printWidth": 120,
  "editor.quickSuggestions": {
    "strings": true
  },
  "cSpell.words": ["WSET"]
}
